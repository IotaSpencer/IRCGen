<%
# <server>
%>
<server
  name="<% @sname %>"
  description="<% @info %>"
  id="<% @id %>"
  network="<% @net %>">
<%
# <admin>
%>
<admin
  name="<% @name %>"
  nick="<% @nick %>"
  email="<% @e_mail %>">
<%
# <connect>
%>
<connect
  allow="*"
  maxchans="<% @maxchans %>"
  timeout="<% @timeout %>"
  pingfreq="<% @pingfreq %>"
  sendq="<% @sendq %>"
  recvq="<% @recvq %>"
  localmax="<% @localmax %>"
  globalmax="<% @globalmax %>"
  useident="no"
  limit="5000"
  modes="<% @umodes %>">
